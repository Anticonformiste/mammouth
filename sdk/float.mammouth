{{
class float
  public float operator+(float other) inline ->
    native("$1 + $2", this, other)
  public float operator-(float other) inline ->
    native("$1 - $2", this, other)
  public float operator*(float other) inline ->
    native("$1 * $2", this, other)
  public float operator/(float other) inline ->
    native("$1 / $2", this, other)
  public float prefix- inline ->
    native("-$1", this)
}}